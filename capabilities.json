{
    "dataRoles": [
        {
            "displayName": "X Axis",
            "name": "x_axis",
            "kind": "Grouping"
        },
        {
            "displayName": "Y Axis",
            "name": "y_axis",
            "kind": "Measure"
        }
    ],
    "objects": {
        "dataPoint": {
            "displayName": "Data colors",
            "properties": {
                "defaultColor": {
                    "displayName": "Default color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "showAllDataPoints": {
                    "displayName": "Show all",
                    "type": {
                        "bool": true
                    }
                },
                "fill": {
                    "displayName": "Fill",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "fillRule": {
                    "displayName": "Color saturation",
                    "type": {
                        "fill": {}
                    }
                },
                 "fontSize": {
                    "displayName": "Text Size",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                }
            }
        }
    },
    "dataViewMappings": [{
        "categorical": {
            "categories": {
                "for": { "in": "x_axis" }
            },
            "values": {
                "select": [
                    { "bind": { "to": "y_axis" } }
                ]
            }
        }
    }]
}
